    title = {{Tip of the Iceberg: Overmassive Black Holes at 4 < z < 7 Found by JWST Are Not Inconsistent with the Local <inline-formula> </inline-formula> Relation}},
    journal = {\apj},
    keywords = {Active galactic nuclei, Quasars, Supermassive black holes, Galaxy evolution, Early universe, Active galaxies, 16, 1319, 1663, 594, 435, 17, Astrophysics - Astrophysics of Galaxies},
    year = {2025},
