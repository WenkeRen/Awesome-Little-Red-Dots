    journal = {\apj},
    keywords = {Supermassive black holes, Clustering, Galaxy dark matter halos, Active galactic nuclei, Low-luminosity active galactic nuclei, James Webb Space Telescope, 1663, 1908, 1880, 16, 2033, 2291, Astrophysics of Galaxies, Cosmology and Nongalactic Astrophysics},
    year = {2026},
