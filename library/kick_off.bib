    title = {{A dormant overmassive black hole in the early Universe}},
    journal = {\nat},
    keywords = {Physical Sciences, Astronomical and Space Sciences, Astrophysics - Astrophysics of Galaxies},
    year = {2024},
    month = {December},
    volume = {636},
    number = {8043},
    pages = {594-597},
    abstract = {{Recent observations have found a large number of supermassive black holes already in place in the first few hundred million years after the Big Bang, many of which seem to be overmassive relative to their host galaxy stellar mass when compared with local relation$^{1, 2, 3, 4, 5, 6, 7, 8{\textendash}9}$. Several different models have been proposed to explain these findings, ranging from heavy seeds to light seeds experiencing bursts of high accretion rate$^{10, 11, 12, 13, 14, 15{\textendash}16}$. Yet, current datasets are unable to differentiate between these various scenarios. Here we report the detection, from the JADES survey, of broad H{\ensuremath{\alpha}} emission in a galaxy at z = 6.68, which traces a black hole with a mass of about 4 {\texttimes} {}10$^{8}$M$\_{{\ensuremath{\odot}}}$ and accreting at a rate of only 0.02 times the Eddington limit. The black hole to host galaxy stellar mass ratio is about 0.4{\textemdash}that is, about 1,000 times above the local relation{\textemdash}whereas the system is closer to the local relations in terms of dynamical mass and velocity dispersion of the host galaxy. This object is most likely an indication of a much larger population of dormant black holes around the epoch of reionization. Its properties are consistent with scenarios in which short bursts of super-Eddington accretion have resulted in black hole overgrowth and massive gas expulsion from the accretion disk; in between bursts, black holes spend most of their life in a dormant state.}},
    doi = {10.1038/s41586-024-08210-5},
    archivePrefix = {arXiv},
    eprint = {2403.03872},
    primaryClass = {astro-ph.GA},
    adsurl = {https://ui.adsabs.harvard.edu/abs/2024Natur.636..594J},
    adsnote = {Provided by the SAO/NASA Astrophysics Data System},
    dimensions = {true},
    altmetric = {160424229},
    lrdIndex = {15}
}

@ARTICLE{2024MNRAS.535..853J,
