    journal = {\apj},
    keywords = {Infrared astronomy, Active galactic nuclei, Galaxies, Astronomy data reduction, Infrared photometry, Luminous infrared galaxies, 786, 16, 573, 1861, 792, 946, Astrophysics - Astrophysics of Galaxies},
    year = {2024},
