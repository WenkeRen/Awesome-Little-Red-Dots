    title = {{UNCOVER: JWST Spectroscopy of Three Cold Brown Dwarfs at Kiloparsec-scale Distances}},
    journal = {\apj},
    keywords = {Brown dwarfs, T dwarfs, T subdwarfs, Milky Way stellar halo, Sky surveys, 185, 1679, 1680, 1060, 1464, Astrophysics - Solar and Stellar Astrophysics, Astrophysics - Earth and Planetary Astrophysics, Astrophysics - Astrophysics of Galaxies},
    year = {2024},
