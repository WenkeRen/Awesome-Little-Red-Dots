    title = {{JWST Census for the Mass-Metallicity Star Formation Relations at z = 4-10 with Self-consistent Flux Calibration and Proper Metallicity Calibrators}},
    journal = {\apjs},
    keywords = {Chemical abundances, Galaxy chemical evolution, Galaxy evolution, James Webb Space Telescope, High-redshift galaxies, 224, 580, 594, 2291, 734, Astrophysics - Astrophysics of Galaxies},
